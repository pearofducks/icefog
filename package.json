    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "abdomen": "^1.0.1",
    "jsdom": "^20.0.3",
    "rollup": "^3.6.0",
    "typescript": "^4.9.3",
    "uvu": "^0.5.6"
  },
  "files": [
    "dist/",
